package(default_visibility = ["//visibility:public"])

load("@io_bazel_rules_sass//:defs.bzl",  "sass_binary", "sass_library")


sass_library(
    name="button_lib",
    srcs = glob(["**/_*.scss"])
)

sass_binary(
    name =  "button_scss",
    src = "pa-button.components.scss",
    deps= [ ":button_lib"]
)

